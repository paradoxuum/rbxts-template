{
	"name": "roblox-ts-game",
	"globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
	"emitLegacyScripts": false,
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"TS": {
				"$path": "out/server"
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					},
					"@flamework": {
						"$path": "node_modules/@flamework"
					}
				}
			},
			"Shared": {
				"$path": "out/shared"
			},
			"Client": {
				"$path": "out/client"
			}
		}
	}
}
